@model IEnumerable<EFEntity.tblOfferMap>
<table>
       <tbody>
           @foreach(var item in Model)
           {
          
                <tr>
                    <td> @item.OfferName </td>
                    <td> @Html.ActionLink("Detail", "Details", new {@id = item.ID})</td>
                </tr> 
           }
       </tbody>
   </table>